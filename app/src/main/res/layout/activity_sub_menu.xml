<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SubMenuActivity"
    android:background="@drawable/mainimage">

    <ImageButton
        android:id="@+id/imgBtnAtraction"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_above="@+id/textView5"
        android:layout_alignStart="@+id/imgBtnTour"
        android:background="@null"
        app:srcCompat="@drawable/atractionimage"
        tools:layout_editor_absoluteX="235dp"
        tools:layout_editor_absoluteY="368dp" />

    <ImageButton
        android:id="@+id/imgBtnActivity"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignStart="@+id/imgBtnItinerary"
        android:layout_alignTop="@+id/imgBtnAtraction"
        android:background="@null"
        app:srcCompat="@drawable/activityimage"
        tools:layout_editor_absoluteX="62dp"
        tools:layout_editor_absoluteY="368dp" />

    <ImageButton
        android:id="@+id/imgBtnItinerary"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_alignStart="@+id/imgBtnPackage"
        android:layout_alignTop="@+id/imgBtnTour"
        android:background="@null"
        app:srcCompat="@drawable/itineraryimage"
        tools:layout_editor_absoluteX="235dp"
        tools:layout_editor_absoluteY="210dp" />

    <ImageButton
        android:id="@+id/imgBtnTour"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_alignEnd="@+id/imgBtnProduct"
        android:layout_alignParentTop="true"
        android:layout_marginTop="179dp"
        android:background="@null"
        android:tooltipText="Send an email"
        app:srcCompat="@drawable/tourimage"
        tools:layout_editor_absoluteX="62dp"
        tools:layout_editor_absoluteY="210dp" />

    <ImageButton
        android:id="@+id/imgBtnPackage"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="50dp"
        android:layout_marginTop="50dp"
        android:background="@null"
        app:srcCompat="@drawable/packageimage"
        tools:layout_editor_absoluteX="235dp"
        tools:layout_editor_absoluteY="61dp" />

    <ImageButton
        android:id="@+id/imgBtnProduct"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="50dp"
        android:layout_marginTop="50dp"
        android:background="@null"
        app:srcCompat="@drawable/productimage"
        tools:layout_editor_absoluteX="62dp"
        tools:layout_editor_absoluteY="61dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imgBtnPackage"
        android:layout_marginStart="41dp"
        android:text="Búsqueda por producto" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/imgBtnPackage"
        android:layout_marginEnd="32dp"
        android:text="Búsqueda por paquete" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/imgBtnTour"
        android:layout_marginStart="34dp"
        android:text="Búsqueda por tour" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/imgBtnActivity"
        android:layout_below="@+id/imgBtnItinerary"
        android:text="Búsqueda por itinerario" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="161dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="116dp"
        android:layout_marginStart="19dp"
        android:text="Búsqueda por atracción" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/imgBtnAtraction"
        android:layout_marginEnd="19dp"
        android:text="Búsqueda por actividad" />
</RelativeLayout>